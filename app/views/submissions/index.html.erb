    <% provide(:title, "Report") %>
        <div class="row">
          <div class="col-md-3 col-md-offset-4">
            <h2>Submission Report</h2>
          </div>
        </div>

          <div class="row">
            <div class="col-md-12">

              <table class="table table-striped table-hover table-condensed">
               <thead>
               <tr>
                 <td colspan="2">
               <form>
                 <div class="input-group">
                   <input type="text" class="form-control" placeholder="Search" name="q">
                   <div class="input-group-btn">
                     <button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
                   </div>
                 </div>
               </form></td></tr>
                <tr class="danger">
                      <th>Name</th>
                      <th>Skill</th>
                      <th>Email</th>
                      <th>Phone</th>
                      <th>Location</th>
                      <th>Rate</th>
                      <th>Availability</th>
                      <th>Relocation</th>
                      <th>Visa</th>
                 </tr>
                </thead>

                <tbody>
                <% @submissions.each do |submission| %>
                    <tr class="success">
                      <td><%= submission.name %></td>
                      <td><%= submission.skill %></td>
                      <td><%= submission.email %></td>
                      <td><%= submission.phone %></td>
                      <td><%= submission.location %></td>
                      <td><%= submission.rate %></td>
                      <td><%= submission.availability %></td>
                      <td><%= submission.relocation %></td>
                      <td><%= submission.visa %></td>

                      <span class="admin">
                          <td><%= link_to time_ago_in_words(submission.created_at) + " ago", submission,  class: "btn btn-xs" %> |
                          <%= link_to 'Edit', edit_submission_path(submission),  class: "btn btn-xs btn-success" %> |
                          <%= link_to 'Destroy', submission, class: "btn btn-xs btn-danger", method: :delete, data: { confirm: 'Are you sure?' } %></td>
                      </span>
                    </tr>
                <% end %>
                </tbody>
              </table>
            </div>
        </div>